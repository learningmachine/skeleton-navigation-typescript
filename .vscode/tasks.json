{
	"version": "0.1.0",
	"command": "npm",
	"isShellCommand": true,
	"showOutput": "always",
	"suppressTaskName": true,
	"tasks": [
		{
			"taskName": "run",
			"args": [ "run", "dev" ],
			"isWatching": true
		},
		{
			"taskName": "build",
			"args": [ "run", "build" ],
			"isBuildCommand": true
		},
		{
			"taskName": "install",
			"args": ["install"]
		},
		{
			"taskName": "update",
			"args": ["update"]
		},
		{
			"taskName": "test",
			"args": ["run", "test"]
		}
	]
}